<?xml version="1.0" encoding="utf-8"?>
<com.cicinnus.cateye.view.ProgressLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/layout_normal_back_title" />

    <android.support.design.widget.TabLayout
        app:tabTextColor="@color/text_primary"
        app:tabSelectedTextColor="@color/colorAccent"
        app:tabBackground="@color/white"
        android:id="@+id/tab_oversea"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <FrameLayout

        android:id="@+id/fl_container_oversea_movie"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</com.cicinnus.cateye.view.ProgressLayout>
